{{>header}}
<div class="row">
  {{>nav}}
  <div class="col-lg-10">
    <div class="container-form">
      <h3 class="editdataform-title">
        Edit Data Admission
      </h3>
      <div class="form-warapper">
        <form method="POST" action="/concern/{{data._id}}?_method=PUT">
      <div class="form-group">
        <label for="title">Title Connern</label>
        <input type="text" value="{{data.title}}" class="form-control" id="title" name="title" required />
      </div>
      <div class="form-group">
        <label for="desc">Description Data</label>
        <textarea type="text" class="form-control" id="desc" name="desc" required >{{data.desc}}</textarea>
      </div>
      <div class="modal-footer">
        <button type="submit" class="btn btn-primary">Add Data</button>
        <a href="/concern/list">
          <button
            type="button"
            class="btn btn-secondary"
            data-dismiss="modal"
          >Close</button>
        </a>
      </div>
    </form>
      </div>
    </div>
  </div>
</div>



